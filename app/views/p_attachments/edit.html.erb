<h1>Editing p_attachment</h1>

<% if Rails::VERSION::STRING < "4" %>
	<% form_for(@p_attachment) do |f| %>
	    <%= f.error_messages %>

	    <p>
	      <%= f.submit 'Update' %>
	    </p>
	<% end %>
<% else %>
    <%= render 'p_attachments/edit_rails4' %>
<% end %>

<%= link_to 'Show', @p_attachment %> |
<%= link_to 'Back', p_attachments_path %>
