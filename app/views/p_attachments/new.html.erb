<h1>New p_attachment</h1>

<% if Rails::VERSION::STRING < "4" %>
	<% form_for(@p_attachment) do |f| %>
	    <%= f.error_messages %>

	    <p>
	      <%= f.submit 'Create' %>
	    </p>
	<% end %>
<% else %>
    <%= render 'p_attachments/new_rails4' %>
<% end %>

<%= link_to 'Back', p_attachments_path %>
